    <messaging clockSkew="00:10:00" lifetime="00:03:00" strict="true">
      <untrustedWebRequest timeout="00:00:10" readWriteTimeout="00:00:01.500" maximumBytesToRead="1048576" maximumRedirections="10">
        <whitelistHosts>
